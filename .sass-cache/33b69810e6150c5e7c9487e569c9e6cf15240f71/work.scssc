#image-scroller {
  top: 0;
  left: 0;
  width: 100%;
  position: absolute;
  z-index: 0;
  overflow: hidden;

  .photo-row {
    float: left;
    width: 5000px;
    margin-bottom: 2px;
    position: relative;

    img {
      height: 100%;
    }

    &.one {
      height: 80px;
      @include opacity(0.1, 10);
      @include animation(slide, 240s, linear, infinite, 0);
    }
    &.two {
      height: 60px;
      @include opacity(0.06, 6);
      @include animation(slide, 320s, linear, infinite, 0);
    }
    &.three {
      height: 43px;
      @include opacity(0.03, 3);
      @include animation(slide, 400s, linear, infinite, 0);
    }
    &.four {
      height: 35px;
      @include opacity(0.02, 2);
      @include animation(slide, 480s, linear, infinite, 0);
    }
    &.five {
      height: 20px;
      @include opacity(0.01, 1);
      @include animation(slide, 600s, linear, infinite, 0);
    }
  }
}

