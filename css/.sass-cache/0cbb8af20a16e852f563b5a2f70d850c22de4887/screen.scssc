:@template"Œ+@import "reset";
@import "mixins";

$linkblue: #5692bc;
$column: 56px;
$wide-width: 1002px;
$narrow-width: 100%;

html,
body {
  color: #fff;
  font-size: 1em;
  background-color: #a6001b;
  @include sentinel-font;
}

a:link,
a:visited {
  color: #a6001b;
  text-decoration: none;
  @include transition(color, 150ms, ease-in-out);
}

a:hover {
  color: #555;
}

/* Touch Styles -----------------------*/
body.touch {
  #intro {
    position: relative;
    left: auto;
    top: auto;
    margin: 0;

    .app-store-info {
      margin-top: 3em;
    }
  }


  #follow-feature,
  #recommend-feature,
  #save-feature,
  #editorschoice-feature {
    border-top: 1px solid #d22c2c;
    position: relative;
    z-index: 1;
    opacity: 1;
    margin: 0 auto;
    left: auto;
    top: auto;
    margin-top: 0;
    display: block;
    text-align: center;
    padding: 4em 0 3.5em;
  }

  #save-feature {
    padding-bottom: 5em;
  }

  #editorschoice-feature {
    margin: 0 auto;
    padding: 6em 0;
  }

  .article-list {
    display: block;
  }

  #container {
    float: left;
    width: 100%;
    background-color: #a6001b;
  }
}

.couch-icon {
  display: block;
  @include readabilicons;
}

#intro {
  z-index: 9;
  width: 100%;
  padding: 8em 0 6em;

  .couch-icon {
    font-size: 2.4em;
    margin: 0em auto -.1em;
    color: white;
  }

  overflow: hidden;
  text-align: center;

  h1 {
    font-size: 3em;
    font-weight: bold;
    color: white;
    margin: .1em 0;
  }

  h2 {
    line-height: 1.3;
    font-size: 1.4em;
    width: 90%;
    color: #fff;
    margin: 0 auto;
  }

  .app-store-info {
    margin: 6em auto 0;
    width: 100%;

    a {
      float: none;
      width: 135px;
      height: 40px;
    }

    p {
      float: none;
      display: block;
      color: #fff;
      border: none;
      margin: .7em 0;
      padding: 0;
      @include whitney-font;
    }
  }

  .device-image {
    margin-top: 3em;
    width: 900px;
    height: auto;
    position: relative;
  }
}

#scroll-instructions {
  float: left;
  width: 100%;
  display: block;
  text-align: center;
  font-size: 1.2em;
  color: #e94c53;
  margin: .6em auto;
  border: none;
  @include sentinel-font;
}

#download-link {
  width: 140px;
  display: block;
  margin: 0 auto;

  img {
    width: 100%;
  }
}

.article-list {
  position: relative;
  font-size: .4em;
  display: none;

  li {
    width: 20px;
    height: 30px;
    text-align: center;
    position: absolute;
    left: 50%;
    top: -50px;
    margin-left: -10px;
    color: #ff565a;
    z-index: 999;
    font-size: 2.2em;
    @include readabilicons;
  }
}

#article-icons {
  display: block;
  font-size: 1em;

  li {
    position: fixed;

    &.hidden {
      opacity: 0;
    }
  }
}

#container {
  position: relative;
}

#follow-feature,
#recommend-feature,
#save-feature,
#editorschoice-feature {
  position: fixed;
  z-index: 1;
  width: 90%;
  top: 50%;
  opacity: 0;
  left: 50%;
  margin-left: -45%;
  margin-top: -175px;
  display: block;
  text-align: center;
  font-size: 2.6em;
  @include transition(opacity, 400ms, ease-in-out);

  &.visible {
    opacity: 1;
  }

  &.fixed {
    position: absolute;
  }

  p {
    width: 90%;
    margin: 0 auto;
    font-size: .5em;
    line-height: 1.4;
  }
}

#editorschoice-feature {
  margin-left: -45%;
  font-size: 2em;
  margin-top: -140px;
  background: transparent url(../images/badge.svg) center center no-repeat;
  background-size: 100% auto;
  padding: 3em 0;

  p {
    color: #e94c53;
    @include whitney-font;
  }
}

.section-icon {
  margin: 0 auto .2em;
  font-size: 3.4em;
  display: block;
  @include readabilicons;
}

#follow-feature {
  .section-icon {
    margin-left: .1em;
  }
}

#recommend-feature {
  .section-icon {
    margin-left: -.7em;
  }
}

.app-store-info {
  width: 300px;
  margin: 0 auto;

  a {
    display: inline-block;

    &:hover {
      color: #d47979;
    }
  }

  p {
    float: left;
    padding: .4em 0 .4em .7em;
    border-left :1px solid #e1e1e1;
    color: #999;
    font-size: .96em;
    display: inline-block;
    margin-left: .7em;
  }
}

#follow-article-icons {
  li {
    &.article-1 {
      margin-left: -130px;
      margin-top: 100px;
    }

    &.article-2 {
      margin-left: -110px;
      margin-top: 10px;
    }

    &.article-3 {
      margin-left: -30px;
      margin-top: 30px;
    }

    &.article-4 {
      margin-left: 55px;
      margin-top: 20px;
    }

    &.article-5 {
      margin-left: 90px;
      margin-top: 120px;
    }
  }
}

#recommend-article-icons {
  li {
    &.article-1 {
      margin-left: 30px;
      margin-top: 115px;
    }

    &.article-2 {
      margin-left: 70px;
      margin-top: -20px;
    }

    &.article-3 {
      margin-left: 10px;
      margin-top: 40px;
    }

    &.article-4 {
      margin-left: 80px;
      margin-top: 70px;
    }

    &.article-5 {
      margin-left: 100px;
      margin-top: 130px;
    }
  }
}

#save-article-icons {
  li {
    &.article-1 {
      margin-left: -145px;
      margin-top: 100px;
    }

    &.article-2 {
      margin-left: -105px;
      margin-top: 10px;
    }

    &.article-3 {
      margin-left: -15px;
      margin-top: -30px;
    }

    &.article-4 {
      margin-left: 75px;
      margin-top: 10px;
    }

    &.article-5 {
      margin-left: 110px;
      margin-top: 100px;
    }
  }
}

#product-heros {
  float: left;
  width: 100%;
  background: #fdfdfd;
  position: relative;
  z-index: 10;
  height: 550px;
  overflow: hidden;

  img {
    position: absolute;
    left: 50%;
  }

  h1 {
    padding: 1.5em 0 .3em;
    color: #353535;
    text-align: center;
    font-size: 2.2em;

    span {
      font-weight: bold;
      color: #a6001b;
      @include sentinel-font;
    }
  }
}

#hero1 {
  margin-left: -150px;
  top: 165px;
  width: 300px;
  z-index: 10;
}

#hero2 {
  margin-left: -175px;
  top: 174px;
  width: 300px;
  z-index: 9;
  display: none;
}

.download-link {
  float: left;
  font-size: 0;
  text-indent: -9999px;
  width: 100px;
  height: 27px;
  background: transparent url(../images/appstorebadge.svg) center center no-repeat;
  background-size: 100% auto;
  display: inline-block;
}

footer {
  float: left;
  width: 100%;
  background: #fdfdfd;
  position: relative;
  z-index: 10;

  ul {
    border-top: 1px solid #e8e8e8;
    margin: 0 auto;
    width: 90%;
    padding: 1em 0;
  }

  li {
    display: block;
    padding: .6em 0;
    font-size: 1.6em;
    text-align: center;

    &:first-child {
      color: #353535;
      font-size: 1.8em;
      @include readabilicons;
    }
  }
}

.feedback-link {
  margin-right: 0.3em;
}

@media screen and (min-width: 520px) {
  #follow-feature,
  #recommend-feature,
  #save-feature,
  #editorschoice-feature {
    p {
      width: 60%;
    }
  }

  #editorschoice-feature {
    width: 60%;
    margin-left: -30%;
  }
}

@media screen and (min-width: 748px) {
  #intro {
    padding: 11em 0 9em;

    .couch-icon {
      font-size: 4em;
      margin: 0em auto -.1em;
    }

    h1 {
      font-size: 6em;
      margin-bottom: .1em;
    }

    h2 {
      font-size: 2em;
      width: 60%;
      margin: 0 auto;
    }

    .app-store-info {
      margin: 8em auto 0;
      width: 380px;
    }

    .device-image {
      margin-top: 3em;
      width: 900px;
    }
  }

  #download-link {
    width: 140px;
  }

  .article-list {
    font-size: .6em;
    display: none;

    li {
      width: 30px;
      height: 50px;
      top: -50px;
      margin-left: -15px;
      font-size: 2.7em;
    }
  }

  #follow-feature,
  #recommend-feature,
  #save-feature,
  #editorschoice-feature {
    width: 90%;
    margin-left: -45%;
    margin-top: -205px;
    font-size: 2.6em;

    p {
      width: 45%;
      margin: 0 auto;
      font-size: .7em;
    }
  }

  #editorschoice-feature {
    width: 70%;
    margin-left: -35%;
    font-size: 3em;
    margin-top: -270px;
    padding: 3em 0;
  }

  .section-icon {
    margin: 0 auto .2em;
    font-size: 6em;
  }

  #follow-feature {
    .section-icon {
      margin-left: .1em;
    }
  }

  #recommend-feature {
    .section-icon {
      margin-left: -.8em;
    }
  }

  #product-heros {
    height: 970px;

    h1 {
      padding: 1.5em 0 .2em;
      font-size: 5em;
    }
  }

  .app-store-info {
    width: 530px;
    margin: 0 auto;

    p {
      padding: .4em 0 .4em .9em;
      font-size: 1.4em;
      margin-left: 1em;
    }
  }

  #hero1 {
    margin-left: 230px;
    top: 357px;
    width: 240px;
  }

  #hero2 {
    margin-left: -470px;
    top: 304px;
    width: 820px;
    display: block;
  }

  .download-link {
    width: 135px;
    height: 40px;
  }

  footer {
    ul {
      width: 90%;
      padding: 2em 0;
    }

    li {
      margin-left: 0.6em;
      font-size: 1em;
      display: inline-block;

      &:first-child {
        font-size: 1.4em;
      }
    }
  }

  .feedback-link {
    float: right;
    margin-right: 0.3em;
  }

  body.touch {
    #intro {
      position: relative;
      left: auto;
      top: auto;
      margin: 0;
    }

    #follow-feature,
    #recommend-feature,
    #save-feature,
    #editorschoice-feature {
      p {
        width: 40%;
      }
    }

    #save-feature {
      padding-bottom: 3em;
      border-bottom: 1px solid #d22c2c;
    }

    #editorschoice-feature {
      border-top: none;
      margin: 0 auto;
      padding: 7em 0;
    }

    .article-list {
      display: block;
    }

    #product-heros {
      float: left;
      background: #fdfdfd;
      position: relative;
      z-index: 10;
      height: 950px;
      overflow: hidden;
    }

    #container {
      float: left;
      width: 100%;
      background-color: #a6001b;
    }

    #follow-article-icons {
      display: block;
    }
  }

  #follow-article-icons {
    li {
      &.article-1 {
        margin-left: -190px;
        margin-top: 150px;
      }

      &.article-2 {
        margin-left: -150px;
        margin-top: 10px;
      }

      &.article-3 {
        margin-left: -30px;
        margin-top: 20px;
      }

      &.article-4 {
        margin-left: 90px;
        margin-top: 0px;
      }

      &.article-5 {
        margin-left: 130px;
        margin-top: 160px;
      }
    }
  }

  #recommend-article-icons {
    li {
      &.article-1 {
        margin-left: 40px;
        margin-top: 175px;
      }

      &.article-2 {
        margin-left: 130px;
        margin-top: -20px;
      }

      &.article-3 {
        margin-left: 30px;
        margin-top: 40px;
      }

      &.article-4 {
        margin-left: 98px;
        margin-top: 96px;
      }

      &.article-5 {
        margin-left: 160px;
        margin-top: 190px;
      }
    }
  }

  #save-article-icons {
    li {
      &.article-1 {
        margin-left: -210px;
        margin-top: 100px;
      }

      &.article-2 {
        margin-left: -150px;
        margin-top: 10px;
      }

      &.article-3 {
        margin-left: -30px;
        margin-top: -30px;
      }

      &.article-4 {
        margin-left: 90px;
        margin-top: 10px;
      }

      &.article-5 {
        margin-left: 160px;
        margin-top: 100px;
      }
    }
  }
}



