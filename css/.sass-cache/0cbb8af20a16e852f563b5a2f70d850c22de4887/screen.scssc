@import "mixins";

$linkblue: #5692bc;
$column: 56px;
$wide-width: 1002px;
$narrow-width: 100%;

html,
body {
  color: #fdfdfd;
  font-size: 1em;
  background-color: #a6001b;
  @include sentinel-font;
}

a:link,
a:visited {
  color: #a6001b;
  text-decoration: none;
  @include transition(color, 150ms, ease-in-out);
}

a:hover {
  color: #555;
}

h1, h2, h3 {
  -webkit-font-smoothing:antialiased;

  &:before {
    -webkit-font-smoothing: subpixel-antialiased;
  }
}

/* Touch Styles -----------------------*/
body.touch {
  #intro,
  #follow-feature,
  #recommend-feature,
  #save-feature,
  #editorschoice-feature {
    display: none;
  }

  #product-heros {
    height: 1780px;
    border-top: 5px solid #a6001b;

    h2 {
      display: block;
      color: #555;
      font-size: 1.2em;
      text-align: center;
    }
  }

  #feature-rundown {
    display: block;
    width: 90%;
    margin: 0 auto 0;
    text-align: center;

    li {
      display: block;
      width: 90%;
      margin: 0 auto;
      padding: 1.3em 0;
      text-align: center;
    }

    span {
      @include readabilicons;
      color: #a6001b;
      font-size: 4em;
    }

    p {
      margin-top: .9em;
      line-height: 1.4;
      font-size: 1.2em;
      color: #555;
    }
  }

  #featured-banner-content {
    width: 256px;
    background: transparent url(../images/badge_red.svg) center center no-repeat;
    background-size: 100% auto;
    padding: 5em 0;
    margin: 670px auto 0;
    text-align: center;
    color: #333;

    h3 {
      font-size: 1.5em;
    }

    p {
      display: block;
      color: #999;
      margin-top: .5em;
      font-size: .9em;
      font-family: Georgia;
      font-weight: 100;
      text-transform: uppercase;
    }
  }

  .app-store-info.first {
    display: none;
  }

  .app-store-info.touch {
    display: block;
    margin-top: 4em;

    p {
      display: none;
    }
  }

  #hero1 {
    top: 145px;
  }

  #hero2 {
    top: 154px;
  }
}

#feature-rundown,
#featured-banner,
.app-store-info.touch {
  display: none;
}

.couch-icon {
  display: block;
  @include readabilicons;
  -webkit-font-smoothing:antialiased;

  &:before {
    -webkit-font-smoothing: subpixel-antialiased;
  }
}

.arrow-icon {
  display: block;
  margin: 2.5em auto 0;
  width: 20px;
  height: 20px;
  border-right: 2px solid #fdfdfd;
  border-top: 2px solid transparent;
  border-bottom: 2px solid #fdfdfd;
  border-left: 2px solid transparent;
  @include rotate(45deg);
  @include animation(pulse, 2s, infinite, ease-in-out, 0);
}

#intro {
  z-index: 9;
  width: 100%;
  padding: 8em 0 6em;

  .couch-icon {
    font-size: 2.4em;
    margin: 0em auto -.1em;
    color: white;
  }

  overflow: hidden;
  text-align: center;

  h1 {
    font-size: 3em;
    font-weight: bold;
    color: white;
    margin: 0;
  }

  h2 {
    line-height: 1.3;
    font-size: 1.4em;
    width: 90%;
    color: #fdfdfd;
    margin: 0 auto;
  }

  .app-store-info {
    margin: 6em auto 0;
    width: 320px;
    @include overflowfix;

    a {
      float: left;
      width: 135px;
      height: 40px;
    }

    p {
      float: left;
      display: block;
      color: #fdfdfd;
      border: none;
      font-size: .9em;
      line-height: 1.4;
      margin: 0 1em 0 10px;
      padding: .1em 1em .1em 20px;
      border-right: 1px solid #db1f3e;
      text-align: right;

      span {
        display: block;
      }
    }
  }

  .device-image {
    margin-top: 3em;
    width: 900px;
    height: auto;
    position: relative;
  }
}

#scroll-instructions {
  float: left;
  width: 100%;
  display: block;
  text-align: center;
  font-size: 1.2em;
  color: #e94c53;
  margin: .6em auto;
  border: none;
  @include sentinel-font;
}

#download-link {
  width: 140px;
  display: block;
  margin: 0 auto;

  img {
    width: 100%;
  }
}

.article-list {
  position: relative;
  font-size: .4em;
  display: none;

  li {
    width: 20px;
    height: 30px;
    text-align: center;
    position: absolute;
    left: 50%;
    top: -50px;
    margin-left: -10px;
    color: #ff565a;
    z-index: 999;
    font-size: 2.2em;
    @include readabilicons;
  }
}

#article-icons {
  display: block;
  font-size: 1em;

  li {
    position: fixed;

    &.hidden {
      opacity: 0;
    }
  }
}

#container {
  position: relative;
}

#follow-feature,
#recommend-feature,
#save-feature,
#editorschoice-feature {
  position: fixed;
  z-index: 1;
  width: 90%;
  top: 50%;
  opacity: 0;
  left: 50%;
  margin-left: -45%;
  margin-top: -175px;
  display: block;
  text-align: center;
  font-size: 2.6em;
  @include transition(opacity, 400ms, ease-in-out);

  &.visible {
    opacity: 1;
  }

  &.fixed {
    position: absolute;
  }

  p {
    width: 90%;
    margin: .1em auto;
    font-size: .5em;
    line-height: 1.4;
  }
}

#editorschoice-feature {
  margin-left: -45%;
  font-size: 2em;
  margin-top: -140px;
  background: transparent url(../images/badge.svg) center center no-repeat;
  background-size: 100% auto;
  padding: 3em 0;

  p {
    color: #e94c53;
    font-family: Georgia;
    font-weight: 100;
    text-transform: uppercase;
  }
}

.section-icon {
  margin: 0 auto .2em;
  font-size: 3.4em;
  display: block;
  @include readabilicons;
}

#follow-feature {
  .section-icon {
    margin-left: .1em;
  }
}

#recommend-feature {
  .section-icon {
    margin-left: -.7em;
  }
}

.app-store-info {
  width: 300px;
  margin: 0 auto;

  a {
    display: inline-block;

    &:hover {
      color: #d47979;
    }
  }

  p {
    float: left;
    padding: .4em 0 .4em .7em;
    border-left :1px solid #e1e1e1;
    color: #999;
    font-size: .96em;
    display: inline-block;
    margin-left: .7em;
  }
}

#follow-article-icons {
  li {
    &.article-1 {
      margin-left: -130px;
      margin-top: 100px;
    }

    &.article-2 {
      margin-left: -110px;
      margin-top: 10px;
