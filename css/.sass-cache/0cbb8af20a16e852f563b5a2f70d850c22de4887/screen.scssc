@media screen and (min-width: 1002px) {
  #intro {
    .couch-icon {
      font-size: 5em;
    }

    h1 {
      font-size: 8em;
    }

    h2 {
      font-size: 2.6em;
    }
  }

  #product-heros {
    height: 960px;

    h1 {
      padding: 1em 0 .1em;
      font-size: 6em;
    }
  }

  #hero1 {
    margin-left: 230px;
