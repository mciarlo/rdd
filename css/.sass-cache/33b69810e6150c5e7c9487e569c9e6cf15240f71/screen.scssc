.back-to-home {
  margin-left: 20px;
  font-size: 0;
  text-indent: -9999px;
  display: block;
  width: 47px;
  height: 19px;
  background: transparent url(../images/arc90_logo.svg) top left no-repeat;
  background-size: 47px;

  &.gray {
    margin: 25px auto;
    width: 69px;
    height: 26px;
    background: transparent url(../images/arc90_logo_gray_small.svg) top left no-repeat;
    background-size: 69px;
  }
}

#arc90-intro-heading,
#arc90-contact-heading {
  margin: 0 20px;
  padding: 3.5em 0 5.5em;
  font-size: 1.8em;
  color: #78b9e7;
  font-weight: 200;
  text-transform: uppercase;
  z-index: 100;
  position: relative;

  strong {
    margin-top: -.07em;
    color: white;
    font-weight: 900;
    display: block;
    font-size: 2.05em;
  }
}

#arc90-contact-heading {
  font-size: 1.2em;
  padding: 1.5em 0 3.5em;

  strong {
    font-size: 3.14em;
  }
}

#nav-links {
  display: none;
}

#view-work-button {
  background: #3d7fae;
  color: #94c5e8;
  padding: 1em 3em 1.1em 1.5em;

  .arrow-icon {
    border-color: #94c5e8;
    display: block;
    width: .35em;
    height: .35em;
    position: absolute;
    top: 1.35em;
    right: 1.5em;
  }

  &:hover {
    color: white;

    .arrow-icon {
      border-color: white;
    }
  }
}

#our-clients {
  display: none;
}

#our-address {
  display: block;
  background: #3d80af;
  padding: 0 20px;
  color: #fff;
  @include overflowfix;

  li {
    display: block;
    padding: .5em 0;
    border-bottom: 1px solid #4C8DB1;
  }

  a:link,
  a:visited {
    color: rgb(141, 189, 216);
    padding: 1em 0 1em 25px;
    display: block;
    @include transition(color, 0ms, linear);
  }

  a:hover {
    color: #fff;
  }
}

#twitter-link {
  background: transparent url(../images/twitter_icon.svg) left 1em no-repeat;

  &:hover {
    background-position: left -43px;
  }
}

#address-link {
  background: transparent url(../images/location_icon.svg) left 1em no-repeat;

  &:hover {
    background-position: left -43px;
  }
}

#telephone-link {
  background: transparent url(../images/telephone_icon.svg) left 1em no-repeat;

  &:hover {
    background-position: left -43px;
  }
}

#email-link {
  background: transparent url(../images/email_icon.svg) left 1.1em no-repeat;

  &:hover {
    background-position: left -43px;
  }
}

#more-from-arc90 {
  padding: 0 20px;
  overflow: hidden;

  h2 {
    text-transform: uppercase;
    margin: 2.5em 0 1em 0;
    font-size: 1.5em;
    font-weight: 600;
  }
}

#featured-lab-project {
  position: relative;
  width: 100%;

  .lab-project-intro {
    width: 60%;
  }

  h3 {
    font-size: 1.4em;
  }

  p {
    font-size: 1.1em;
    line-height: 1.2;
    color: #888;
    margin: .5em 0 1em;
  }

  .arrow-link {
    padding-right: 1em;
    color: #888;
    line-height: 1.3;

    &:hover {
      color: $linkblue;

      .arrow-icon {
        border-color: $linkblue;
      }
    }
  }

  .arrow-icon {
    border-color: #999;
    display: block;
    width: .35em;
    height: .35em;
    position: absolute;
    bottom: .35em;
    right: 0;
  }

  .featured-lab-project-image {
    position: absolute;
    bottom: 0;
    left: 65%;
    height: 175px;
    overflow: hidden;

    img {
      display: block;
      width: 420px;
    }
  }
}

#new-blog-posts  {
  margin-top: 1em;
}

#new-blog-posts li {
  border-bottom: 1px solid #eaeaea;
  padding: 15px 0px;
  position: relative;

  &:first-child {
    border-top: 1px solid #eaeaea;
  }

  .user-avatar {
    border: 4px solid #d3d3d3;
    @include border-radius(200px);
    overflow: hidden;
    width: 50px;
    height: 50px;
    position: absolute;
    top: 15px;
    right: 0;

    img {
      width: 100%;
      height: auto;
    }
  }
}

.post-title {
  display: inline-block;
  font-size: 1.5em;
  margin: 0 60px .6em 0;
}

.post-byline {
  display: block;
  color: #999;
  text-transform: uppercase;

  em {
    font-style: italic;
    text-transform: none;
  }
}

#more-blog-posts {
  clear: both;
  float:left;
  padding: .3em 1em .3em 0;
  color: #999;
  margin: 2em 0 5em 0;
  display: inline-block;

  &:hover {
    color: $linkblue;

    .arrow-icon {
      border-color: $linkblue;
    }
  }

  .arrow-icon {
    border-color: #999;
    display: block;
    width: .35em;
    height: .35em;
    position: absolute;
    top: .6em;
    right: 0;
  }
}

footer {
  float:left;
  background: #f1f1f1;
  width: 100%;
  text-align: center;
}

#footer-links {
  @extend #nav-links;
  display: block;

  a:link,
  a:visited {
    display: block;
    color: #999;
    padding: 1.2em 20px;
    font-size: 1.5em;
    font-weight: 100;
    border-top: 1px solid #e1e1e1;
  }

  &.project-page {
    a:link,
    a:visited {
      border-color: rgb(53, 56, 58);
      font-size: 1.3em;
      padding: 1em 20px;
    }

    a:hover {
      color: white;
    }
  }
}

#contact-us {
  position: relative;
  background: #f1f1f1;
  z-index: 1;
  overflow: hidden;
  padding: 2em 20px 3em;
  border-bottom: 1px solid #e1e1e1;

  .error-message {
    display: none;
    background: #eceabf;
    padding: .4em 0;
    width: 100%;
    float: left;
  }

  h2 {
    text-align: center;
    font-weight: 600;
    font-size: 1.6em;
    margin: 0 auto;
    z-index: 1;
    position: relative;
    @include whitney-font;
    line-height: 1.1;
  }

  p {
    text-align: center;
    font-size: .9em;
    line-height: 1.4;
    margin: .5em auto 1.5em auto;
    z-index: 1;
    position: relative;
    @include whitney-font;

    a:link,
    a:visited {
      color: #d98f3f;
    }

    a:hover {
      color: #555;
    }
  }

  input[type=text],
  input[type=email],
  textarea {
    background: #fff;
    border: 2px solid #e1e1e1;
    font-size: 1.2em;
    padding: .5em 1em .6em;
    outline: none;
    width: 100%;
    display: block;
    margin: 0 0 1em 0;
    @include whitney-font;
    @include box-sizing;
    -webkit-appearance: none;

    &:focus {
      border-color: #9FBFD5;
    }
  }

  textarea {
    height: 200px;
    resize: vertical;
  }
}

#contact-form {
  text-align: center;

  li {
    float: left;
    width: 100%;
  }

  label {
    display: block;
    text-align: left;
    display: none;
    @include whitney-font;
    margin-bottom: .5em;
    font-size: 1.1em;
  }
}

#contact-submit {
  -webkit-appearance: none;
  display: inline-block;
  background: #4783ab;
  border: none;
  cursor: pointer;
  padding: .4em 1.5em .5em;
  color: white;
  font-size: 1.2em;
  @include border-radius(2em);
  @include transition(background-color, 150ms, ease-in-out);

  &:hover {
    background: #555;
  }

  .state {
    display: none;
  }

  .state.default {
    display: block;
  }

  &.error,
  &.sending,
  &.sent {
    .state.default {
      display: none;
    }
  }

  &.error {
    .state.error {
      display: block;
    }
  }

  &.sending {
    .state.sending {
      display: block;
    }
  }

  &.sent {
    .state.sent {
      display: block;
    }
  }
}

#error-actions {
  position: absolute;
  top: 38%;
  width: 100%;
  text-align: center;
}

#error-page-heading {
  width: 100%;
  color: white;
  font-size: 1.6em;
  margin-bottom: .5em;
}

#error-page-home-btn {
  font-size: .7em;
  border: 2px solid #cbcbcb;
  text-transform: uppercase;
  padding: 10px 15px;
  color: #ababab;
  background: none;
  display: inline-block;
  @include border-radius(50px);
  cursor: pointer;
  @include whitney-font;
  transition: color 150ms, border-color 150ms ease-in-out;
  -webkit-transition: color 150ms, border-color 150ms ease-in-out;
  -moz-transition: color 150ms, border-color 150ms ease-in-out;
  -ms-transition: color 150ms, border-color 150ms ease-in-out;
  text-align: center;
  border-color: #3d7fae;
  color: #579FD1;
  padding: .7em 2em;
}

#error-page-home-btn:hover {
  color: #fff;
  border-color: #fff;
}

@media screen and (min-width: 380px) {
  #featured-lab-project {
    .featured-lab-project-image {
      img {
        width: 340px;
      }
    }
  }
}

@media screen and (min-width: 640px) {
  #default-header {
    background-size: auto 120%;
  }
}

