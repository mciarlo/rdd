      }

      img {
        position: absolute;
        bottom: -100px;
        width: 100%;
        left: 0;
        @include transition(bottom, 150ms, ease-in-out);
      }
    }
  }

  #new-blog-posts li {
    width: 25%;
    border-left: 1px solid #eaeaea;
    border-bottom: none;
    padding: 0 25px;
    float:left;
    @include box-sizing;

    &:first-child {
      border-left: none;
      border-top: none;
      padding-left: 0;
    }

    .user-avatar {
      display: block;
      width: 55px;
      height: 55px;
      top: auto;
      right: auto;
      position: relative;

      img {
        width: 100%;
        height: auto;
      }
    }
  }

  .post-title {
    font-size: 1.4em;
    margin: .3em 0 .6em 0;
  }

  .post-byline {
    color: #999;
    font-size: .8em;
    text-transform: uppercase;

    em {
      font-style: italic;
      text-transform: none;
    }
  }

  #more-blog-posts {
    .arrow-icon {
      top: .6em;
    }
  }

  #footer-links {
    a:link,
    a:visited {
      display: block;
      color: #999;
      padding: 3em 0;
      font-size: .8em;
      font-weight: 100;
      border: none;
    }

    &.project-page {
      a:link,
      a:visited {
        font-size: .8em;
        padding: 3em 0;
      }

      a:hover {
        color: white;
      }
    }
  }

  footer {
    float:left;
    padding: 0;
    background: #f1f1f1;
    width: 100%;
  }

  #contact-us {
    padding: 4em 0;

    li {
      width: 49%;
      margin-left: 2%;

      &.message,
      &:first-child {
        margin-left: 0;
      }

      &.message {
        width: 100%;
      }
    }

    input[type=text],
    input[type=email],
    textarea {
      float: left;
      font-size: 1.5em;
      width: 100%;
      margin-right: 4%;
    }

    input[type=email] {
      margin-right: 0;
    }

    textarea {
      width: 100%;
    }

    h2 {
      font-size: 2.5em;
    }

    p {
      font-size: 1.2em;
    }
  }

  #contact-submit {
    font-size: 1.3em;
  }

  #error-page-heading {
    font-size: 3em;
  }

  #error-page-home-btn {
    font-size: .8em;
  }
}


@media screen and (min-width: 1002px) {
  .wrapper {
    width: 1002px;
  }

  #default-header {
    &.contact {
      height: 300px;
      background: #264c67 url(../images/arc90_header_map.jpg) center 30% repeat-x;
    }
  }

  #arc90-location-marker {
    margin: -50px 0 0 -50px;
    width: 100px;
    height: 100px;

    img {
        margin: -16px 0 0 -36px;
    }
  }

  #arc90-intro-heading,
  #arc90-contact-heading {
    font-size: 3.4em;

    strong {
      font-size: 3.94em;
    }
  }

  #client-logos,
  #more-from-arc90 {
    width: 1002px;
  }

  .author-portrait {
    width: 65px;
  }

  .post-title {
    font-size: 1.8em;
  }

  .post-byline {
    font-size: 1em;
  }

  #more-from-arc90 {
    h2 {
      font-size: 1.5em;
      margin: 3em 0 1em 0;
    }
  }

  #featured-lab-project {
    .lab-project-intro {
      width: 260px;
      margin-right: 40px;
    }

    h3 {
      font-size: 1.8em;
    }

    p {
      margin: .5em 0 2em 0;
      font-size: 1.1em;
    }

    .featured-lab-project-image {
      width: 550px;
      height: 15.8em;

      &:hover img {
        bottom: -160px;
      }

      img {
        bottom: -170px;
      }
    }
  }

  #new-blog-posts li {
    .user-avatar {
      width: 65px;
      height: 65px;
    }
  }

  .arrow-link {
    font-size: 1em;
  }

  #contact-us {
    h2 {
      font-size: 3.3em;
      width: 80%;
    }

    p {
      font-size: 1.4em;
      width: 70%;
    }
  }

  #contact-submit {
    font-size: 1.5em;
  }

  #error-page-heading {
    font-size: 4em;
  }

  #error-page-home-btn {
    font-size: .9em;
  }
