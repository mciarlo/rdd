:@template"˜S@import "reset";
@import "animations";
@import "mixins";
@import "work";

$linkblue: #5692bc;
$column: 56px;
$wide-width: 1002px;
$narrow-width: 100%;

html,
body {
  color: #454545;
  font-size: 1em;
  @include sentinel-font;
}

a:link,
a:visited {
  color: $linkblue;
  text-decoration: none;
  @include transition(color, 150ms, ease-in-out);
}

a:hover {
  color: #555;
}

.wrapper {
  width: 100%;
}

.arrow-link {
  position: relative;
}

.arrow-icon {
  border-top: 2px solid;
  border-right: 2px solid;
  @include rotate(45deg);
  @include transition(border-color, 150ms, ease-in-out);
}

h1 {
  @include gotham-font;
}

h2, h3, h4, h5, h6,
.post-title,
#nav-links,
.arrow-link,
#view-work-button {
  @include whitney-font;
}

/* Main Heading -------------------------*/
#main-nav,
#arc90-intro-heading,
#view-work-button {
  position: relative;
  z-index: 1;
}

#contact {
  background: #f1f1f1;
}

#default-header {
  position: relative;

  &.contact {
    height: 200px;
    background: #1D445F url(../images/arc90_header_map.jpg) center 35% repeat-x;
    background-size: auto 200%;
  }
}

#arc90-location-marker {
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -40px 0 0 -40px;
  background: white url(../images/arc90_logo_color.svg) center center no-repeat;
  background-size: 75%;
  width: 80px;
  height: 80px;
  text-align: center;
  @include border-radius(100px);
}

#main-nav {
  width: 100%;
  border-bottom: 1px solid #3f505a;
  border-bottom: 1px solid rgba(171,205,229,0.1);
  padding: .8em 0;

  &.contact {
    background: #3d80af;
    border: none;

    a:link,
    a:visited {
      color: rgb(135, 188, 226);
    }

    a:hover,
    a.active {
      color: white;
    }
  }
}

#background-effects {
  background: #1D445F;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;

  &.no-bg {
    background-color: transparent;

    .orb {
      @include opacity(0.4, 40);
    }
  }

  .orb-layer {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 0;
  }
}

.orb {
  background: #225579;
  @include border-radius(100px);
  position: absolute;
  left: 50%;
}

$panningspeed : 45s;

#orb-layer-1 {
  @include animation(panningbig, $panningspeed, linear, infinite, 0);
}

#orb-layer-2 {
  @include opacity(0.9, 90);
  @include animation(panningmedium, $panningspeed, linear, infinite, 0);
}

#orb-layer-3 {
  @include opacity(0.7, 70);
  @include animation(panningsmall, $panningspeed, linear, infinite, 0);
}

#orb-layer-4 {
  @include opacity(0.5, 50);
  @include animation(panningtiny, $panningspeed, linear, infinite, 0);
}

